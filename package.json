{
  "name": "ngx-localstorage-demo",
  "version": "3.1.0",
  "description": "An Angular wrapper for local storage access.",
  "scripts": {
    "ng": "ng",
    "update": "ng update @nrwl/workspace",
    "build:prod": "npm run build:lib && npm run copy && npm run build:docs",
    "build:lib": "ng build ngx-localstorage --prod",
    "copy": "npm run schematics:copy && copyfiles ./LICENSE ./README.md ./dist/libs/ngx-localstorage",
    "schematics:copy": "copyfiles -u 2 ./schematics/src/**/*.js ./schematics/src/**/*.json ./dist/libs/ngx-localstorage/schematics",
    "build:docs": "ng build --prod --output-path docs --base-href ngx-localstorage",
    "schematics:watch": "cd ./schematics/ && npm link && cd .. && npm link schematics && npm run build --prefix ./schematics -- --watch",
    "nx": "nx",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "dependencies": {
    "@angular/animations": "^10.1.4",
    "@angular/cdk": "^10.2.3",
    "@angular/common": "^10.1.4",
    "@angular/compiler": "^10.1.4",
    "@angular/core": "^10.1.4",
    "@angular/forms": "^10.1.4",
    "@angular/material": "^10.2.3",
    "@angular/platform-browser": "^10.1.4",
    "@angular/platform-browser-dynamic": "^10.1.4",
    "@angular/router": "^10.1.4",
    "@nrwl/angular": "9.1.2",
    "core-js": "^2.5.4",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular-devkit/build-ng-packagr": "^0.1001.4",
    "@angular-devkit/core": "^10.1.4",
    "@angular-devkit/schematics": "^10.1.4",
    "@angular/cli": "^10.1.4",
    "@angular/compiler-cli": "^10.1.4",
    "@angular/language-service": "^10.1.4",
    "@nrwl/workspace": "9.1.2",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "copyfiles": "^2.1.1",
    "dotenv": "6.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^10.1.2",
    "prettier": "^1.19.1",
    "protractor": "^5.4.1",
    "ts-node": "~7.0.0",
    "tslint": "6.0.0",
    "typescript": "~4.0.3",
    "webpack": "^4.39.2"
  }
}
