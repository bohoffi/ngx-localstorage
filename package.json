    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-deprecation": "^1.3.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-preset-angular": "13.1.2",
    "ng-packagr": "16.0.0",
    "ngx-doc-gen": "^2.1.1",
    "nx": "16.10.0",
    "postcss": "^8.4.5",
